  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.17.6",
    "@types/pouchdb": "^6.4.0",
    "esbuild": "^0.19.2",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "pino-pretty": "^10.2.0",
    "prettier": "^3.0.2",
    "ts-jest": "^29.1.1",
